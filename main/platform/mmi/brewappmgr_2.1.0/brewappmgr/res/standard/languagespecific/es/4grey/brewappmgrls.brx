	<STRING Id="1017" Name="IDS_BUY_AIRTIME_WARNING">
		<Text>Al conectarse al servidor para comprar el programa, se cobran gastos de conexión.</Text>
	</STRING>
	<STRING Id="1018" Name="IDS_RESTORE_AIRTIME_WARNING">
