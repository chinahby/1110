#===============================================================================
#			      TARGET RULES
#===============================================================================

# The following are the target rules supported by this makefile with the
# exception of the 'depend' target which is located at the end of this file.
ifeq ($(USES_MMI),yes)
#-------------------------------------------------------------------------------
#
# Rules for building EFS File System
#
#-------------------------------------------------------------------------------
ifeq ($(USES_CONSTEFS),yes)
CONSTFILE_BIN_FILES += $(subst fs:/,$(CONSTEFS_DIR)/,$(EFSFILE_FILES))
else
EFSFILE_DIR = $(BINPATH)/$(TARGET)/efs
EFSFILE_FILENAMES  = $(filter-out fs:/%, $(subst |, ,$(EFSFILE_FILES)))
EFSFILE_FSLOC      = $(subst fs:/,$(EFSFILE_DIR)/,$(firstword $(filter fs:/%, $(subst |, ,$(filter $(notdir $<)|%, $(EFSFILE_FILES))))))
EFSFILE_TARGETS    = $(addprefix $(TARGETDIR)/,$(EFSFILE_FILENAMES))

$(EFSFILE_TARGETS) : $(TARGETDIR)/% : %
	@echo ------------------------------------------------------------------------
	@echo Copy $< to $(EFSFILE_FSLOC)
	@if ! test -d $(dir $(EFSFILE_FSLOC)); then mkdir -p $(dir $(EFSFILE_FSLOC)); fi
	@cp $< $@
	@cp $< $(EFSFILE_FSLOC)
	@echo ------------------------------------------------------------------------
endif

#-------------------------------------------------------------------------------
#
# Rules for building Const File System
#
#-------------------------------------------------------------------------------
CONSTFILE_BIN_FILENAMES = $(filter-out FLAGS:%, $(filter-out fs:/%, $(subst |, ,$(CONSTFILE_BIN_FILES))))
CONSTFILE_BIN_SOURCES 	= $(addsuffix .c, $(CONSTFILE_BIN_FILENAMES))
CONSTFILE_BIN_OBJS 		= $(addprefix $(TARGETDIR)/,$(CONSTFILE_BIN_SOURCES:%.c=%.o))

CONSTFILE_FILENAMES 	= $(filter-out FLAGS:%, $(filter-out fs:/%, $(subst |, ,$(CONSTFILE_FILES))))
CONSTFILE_SOURCES 		= $(addsuffix .c, $(CONSTFILE_FILENAMES))

CONSTFILE_FSLOC 		= $(firstword $(filter fs:/%, $(subst |, ,$(filter $(notdir $<)|%, $(CONSTFILE_FILES) $(CONSTFILE_BIN_FILES))))) 

CONSTFILE_FLAGENT 		= $(firstword $(filter FLAGS:%, $(subst |, ,$(filter $(notdir $<)|%, $(CONSTFILE_FILES) $(CONSTFILE_BIN_FILES)))))
CONSTFILE_FLAGS 		= $(subst FIXED,-f,$(subst ., ,$(subst FLAGS:, , $(CONSTFILE_FLAGENT))))

CONSTFILE_OBJS 			= $(addprefix $(TARGETDIR)/,$(CONSTFILE_SOURCES:%.c=%.o)) $(CONSTFILE_BIN_OBJS)
CONSTFILE_BAR_FSLOC 	= $(firstword $(filter fs:/%, $(subst |, ,$(filter $(notdir $(<:%.brx=%.bar))|%, $(CONSTFILE_FILES)))))
CONSTFILE_BAR_FSNAME    = $(notdir $(subst /$(EXTLANG2)/,/$(EXTLANG2),$(subst /$(EXTLANG1)/,/$(EXTLANG1),$(CONSTFILE_BAR_FSLOC))))
CONSTFILE_BAR_BUILD     = $(dir $<)$(CONSTFILE_BAR_FSNAME)
CONSTFILE_BAR  			= $(filter %.bar, $(CONSTFILE_FILENAMES))
CONSTFILE_BAR_SRC 		= $(addprefix $(TARGETDIR)/, $(addsuffix .c, $(CONSTFILE_BAR)))
CONSTFILE_BIN  			= $(filter-out %.bar, $(CONSTFILE_FILENAMES)) $(CONSTFILE_BIN_FILENAMES)
CONSTFILE_BIN_SRC 		= $(addprefix $(TARGETDIR)/, $(addsuffix .c, $(CONSTFILE_BIN)))

 $(TARGETDIR)/OEMConstFiles_Dynamic.c : OEMConstFiles.c $(CONSTFILE_SOURCES) $(CONSTFILE_BIN_SOURCES)
	@echo ------------------------------------------------------------------------
	@echo Generating $@
	@cp $< $@
	@chmod 666 $@
	@perl $(BREWDIR)/pk/utils/OEMConstFileMod.pl $@ $(CONSTFILE_FILENAMES) $(CONSTFILE_BIN_FILENAMES)
	@echo ------------------------------------------------------------------------
	
OBJECTS += $(CONSTFILE_OBJS)

$(CONSTFILE_BIN_SRC) : $(TARGETDIR)/%.c: %
	@echo ------------------------------------------------------------------------
	@echo Creating C File $@
	@$(BREWDIR)/pk/utils/bin2src.exe -s$(subst /,\\,$<) -d$(CONSTFILE_FSLOC) $(CONSTFILE_FLAGS) -o$(subst /,\\,$@)
	@echo ------------------------------------------------------------------------

$(CONSTFILE_BAR_SRC) : $(TARGETDIR)/%.bar.c: %.brx
	@echo ------------------------------------------------------------------------
	@echo Build Resource $(<:%.brx=%.bar)
	@$(BREWRC) -h $(CONSTFILE_BAR_BUILD:%.bar=%.brh) -o $(CONSTFILE_BAR_BUILD) $<
	@-mv -f $(CONSTFILE_BAR_BUILD) $(<:%.brx=%.bar)
	@echo ------------------------------------------------------------------------
	@echo ------------------------------------------------------------------------
	@echo Creating C File $@
	@$(BREWDIR)/pk/utils/bin2src.exe -s$(subst /,\\,$(<:%.brx=%.bar)) -d$(CONSTFILE_BAR_FSLOC) -o$(subst /,\\,$@)
	@echo ------------------------------------------------------------------------
	
$(CONSTFILE_OBJS) : $(TARGETDIR)/%.o : $(TARGETDIR)/%.c
	$(c_compile)
	
.PHONY : mmires
mmires: $(CONSTFILE_BIN_SRC) $(CONSTFILE_BAR_SRC) $(TARGETDIR)/OEMConstFiles_Dynamic.c $(EFSFILE_TARGETS)
	@echo ------------------------------------------------------------------
	@echo BREW RESOURCE BUILDER
	@echo ------------------------------------------------------------------
endif	
