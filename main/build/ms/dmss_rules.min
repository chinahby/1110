define link
	@echo -------------------------------------------------------------------------
	@echo TARGET $(ASIC) $(BUILD) $(VERSION)
	$(CC) -E $(CUSTH) $(TG) $(ARM_ASM) -I. -I../cust -I../../data/sta -I../../gps -I$(UTILS) -I$(MIBIB) -I$(BOOT_COMMON_SHARED_BOOT) $(SCLFILE_IN) > $(SCLFILE)
