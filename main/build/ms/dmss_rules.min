#-------------------------------------------------------------------------------
ifeq ($(USES_JOYST), yes)
	-@chmod +w $(LIBDIR)/varatouch.lib
	$(COPY) $(call slashes,$(JOYST)/varatouch.lib $(LIBDIR)/varatouch.lib)
endif

